# GitHub Copilot Token Configuration
# Multiple tokens can be provided as comma-separated values
# Each token MUST start with 'gho_'
# Example: REFRESH_TOKEN=gho_token1,gho_token2,gho_token3
REFRESH_TOKEN=gho_your_token_here

# Index of the active token (0-based)
# If you have multiple tokens, this controls which one is currently used
# Example: If REFRESH_TOKEN=gho_token1,gho_token2, then:
# ACTIVE_TOKEN_INDEX=0 uses gho_token1
# ACTIVE_TOKEN_INDEX=1 uses gho_token2
ACTIVE_TOKEN_INDEX=0

# Request settings
MAX_TOKENS=10240
TIMEOUT_SECONDS=300
EDITOR_VERSION=vscode/1.97.2

# Rate limiting settings
SLEEP_BETWEEN_CALLS=0

# Debug settings
RECORD_TRAFFIC=false
LOGURU_LEVEL=INFO